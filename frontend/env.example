# Frontend Environment Variables

# Sentry Configuration (optional - leave empty to disable)
NEXT_PUBLIC_SENTRY_DSN=

# Backend API URL
NEXT_PUBLIC_BACKEND_URL=http://localhost:8000/api

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# App URLs
NEXT_PUBLIC_URL=http://localhost:3000
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Google OAuth (optional)
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id

# Environment Mode
NEXT_PUBLIC_ENV_MODE=development

# Vercel Environment (auto-set by Vercel)
NEXT_PUBLIC_VERCEL_ENV=

# Orshot API Key (for OG images)
ORSHOT_API_KEY=your_orshot_api_key

# OpenAI API Key (for server-side operations)
OPENAI_API_KEY=your_openai_api_key 